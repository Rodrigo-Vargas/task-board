<%= form_for @list do |f|  %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :position %>
    <%= f.text_field :position, class: 'form-control' %>
  </div> 

  <div>
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>
